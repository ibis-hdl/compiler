{
   "version": "0.2",
   "language": "en",
   "cache": {
      "useCache": true,
      "cacheLocation": "dictionaries/.cspellcache",
      "cacheStrategy": "content"
   },
   "ignorePaths": [
      ".git",
      ".gitignore",
      "*.log",
      "build/**"
   ],
   "dictionaryDefinitions": [
      {
         "name": "c++-terms",
         "path": "../.dictionaries/cpp_terms.txt"
      },
      {
         "name": "vhdl-terms",
         "path": "../.dictionaries/vhdl_terms.txt"
      },
      {
         "name": "python-terms",
         "path": "../.dictionaries/python_terms.txt"
      },
      {
         "name": "project",
         "path": "../.dictionaries/project_words.txt"
      }
   ],
   "dictionaries": [
      "c++-terms",
      "vhdl-terms",
      "python-terms",
      "project"
   ],
   "languageSettings": [
      {
         "languageId": "c++",
         "dictionaries": [
            "c++-terms"
         ]
      },
      {
         "languageId": "vhdl",
         "dictionaries": [
            "vhdl-terms"
         ]
      },      
      {
         "languageId": "python",
         "dictionaries": [
            "python-terms"
         ]
      },
      {
         "languageId": "plaintext",
         "dictionaries": [
            "project"
         ]
      }
   ]
}
